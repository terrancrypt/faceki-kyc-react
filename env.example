# FACEKI API Configuration
# Copy this file to .env and fill in your actual values

# Your FACEKI API Key (get from FACEKI Dashboard)
VITE_FACEKI_API_KEY=your_api_key_here

# Default redirect URL for KYC verification
VITE_DEFAULT_REDIRECT_URL=https://your-domain.com/callback

# API Base URL (usually don't need to change)
VITE_FACEKI_API_BASE_URL=https://api.faceki.com/v2

# Development settings
VITE_DEV_MODE=true
VITE_ENABLE_LOGGING=true
